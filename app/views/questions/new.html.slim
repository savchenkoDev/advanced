= render 'shared/errors', resource: @question
= form_with model: @question do |f| 
  = f.label :title
  = f.text_field :title
  br
  = f.label :body
  = f.text_area :body
  br
  .attachments
    p Attachments:
    p
      =render 'shared/nested_form', form: f, resource: @question

  = f.submit 'Create'
  br
